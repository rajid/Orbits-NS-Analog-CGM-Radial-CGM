
<symbol id="clock" >
 <svg>
  <svg>  
    <mask id="mask">
      <rect x="50%-1" y="0" width="3" height="250" />
      <rect x="0" y="50%-1" width="250" height="3" />
      <g transform="translate(50%-75,50%-129),rotate(60)">
        <rect x="$" y="$-1" width="250" height="3"  />
      </g>
      <g transform="translate(50%-129,50%-75),rotate(30)">
        <rect x="$" y="$-1" width="250" height="3"  />
      </g>
      <g transform="translate(50%-129,50%+75),rotate(-30)">
        <rect x="$" y="$-1" width="250" height="3"  />
      </g>
      <g transform="translate(50%-75,50%+129),rotate(-60)">
        <rect x="$" y="$-1" width="250" height="3"  />
      </g>
    </mask>  

    <svg  mask="#mask" x="50%-125" y="50%-125" width="250" height="250" >
      <gradientRect id="rays" width="250" height="250" fill="cyan"
      gradient-type="radial"
      gradient-x1="50%" gradient-y1="50%"
      gradient-x2="90%" gradient-y2="90%"
      gradient-color1="yellow"
      gradient-color2="black" />
    </svg>
  </svg>

   <g id="orbit" transform="translate(50%,50%)" opacity="1" >
     <rect x="0" y="-20%" width="1" height="26%" fill="red"  opacity="0" >
       <g id="earth" transform="translate($,-26%)">
          <image href="earth_only.png" width="35" height="35" x="$-18" y="$-18" opacity="1" />
          <g id="moonorbit" opacity="1" >
            <rect x="$-18" y="$-18" width="1" height="35" fill="red" opacity="0" >
              <g id="moon" transform="translate($, -35)">
                <image href="moon_only.png" width="15" height="15" x="$-8" y="$-8" />
              </g>
            </rect>
          </g>
       </g>
    </rect>
  </g>

  <g id="cometorbit" transform="translate(50%,50%)" opacity="1">
      <rect x="0" y="-140" width="0" height="140" >

        <g id="comet" transform="translate($,-140)" display="none">
          <image href="comet.png" width="40" height="30" x="$-3" y="$-16" opacity="1" />
        </g>
      </rect>
  </g>

   <g id="belt" transform="translate(50%,50%)" opacity="1">
      <rect x="0" y="46%" width="0" height="46%" >
        <g id="asteroid" transform="translate($,-46%)">
          <image href="asteroid.png" width="20" height="20" x="$-10" y="$-10" opacity="1" />
        </g>
      </rect>
  </g>

  <image id="star" href="StarOnly.png" x="50%-27" y="50%-27" width="53" height="53" />

</svg>
</symbol>
